{
    "expo": {
        "name": "Orpy",
        "slug": "Orpy",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#1a1a1a"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#000000"
            }
        },
        "ios": {
            "supportsTablet": true
        }
    }
}
